sh /etc/init.d/rc.mc_defaults

if [ $DO_AUTOCONFIG == yes ]
then
	#
	# Default parameters for this platform
	#
	param set BAT_V_SCALING 0.00989
	param set BAT_C_SCALING 0.0124
fi

set MIXER FMU_quad_w

set PWM_OUTPUTS 1234
