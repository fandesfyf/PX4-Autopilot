#!nsh
#
# @name AEROFC V1 board
#
# @type Quadrotor x

sh /etc/init.d/rc.mc_defaults

tap_esc start -d /dev/ttyS0 -n 4
usleep 300000

set OUTPUT_MODE rcin
set OUTPUT_DEV /dev/tap_esc
set MIXER quad_x
set USE_IO no

param set SYS_COMPANION 0
set MAVLINK_F "-b 115200 -mode onboard -r 80000 -d /dev/ttyS1"
