#PWM Hz
set PWM_RATE 50

#PWW default value for "disarmed" mode
set PWM_DISARMED 1500

#PWM range
set PWM_MIN 1200
set PWM_MAX 1800

#enable servo output on pins 3 and 4 (steering and thrust)
set PWM_OUT 34
